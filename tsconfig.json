{
	"module": "es2020",
	"moduleResolution": "node",
	"compilerOptions": {
    	"outDir": "./dev/",
    	"noImplicitAny": false,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules/"
	]
}
