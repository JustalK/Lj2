{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/",
    "dev/",
    "router/",
    "config/"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "osascript -e 'RESTART'"
  },
  "watch": [
    "src/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,vue"
}